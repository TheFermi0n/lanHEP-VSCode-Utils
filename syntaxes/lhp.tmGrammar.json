{
  "scopeName": "source.lhp",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#comments"
    },
    {
      "include": "#operators"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.lhp",
          "match": "\\b(do_if|end_if)\\b"
        },
        {
          "name": "keyword.operator.lhp",
          "match": "\\b(parameter|external_func|keys|model|let|option|lterm)\\b"
        },
        {
          "name": "keyword.math.lhp",
          "match": "\\b(sqrt|acos|tan|sin|cos|AddHermConj)\\b"
        },
        {
          "name": "keyword.function",
          "match": "\\b(vector|spinor|scalar)\\b"
        },
        {
          "include": "#lanHEPConstant"
        }
      ],
      "repository": {
        "lanHEPConstant": {
          "patterns": [
            {
              "name": "constant.character.lhp",
              "match": "\\b(electron|neutrino)\\b"
            },
            {
              "name": "constant.character.lhplabel",
              "match": "\\b(Internal|External|QED|LOOP|QCD|HIG|NP|ANOINPUTS|SMINPUTS|YUKAWA|SU2D|SU2W|Gluon|Colour|Generation|Sine)\\b"
            },
            {
              "name": "constant.character.property",
              "match": "\\b(GhostNumber|LeptonNumber|FlavorExpand)\\b"
            },
            {
              "name": "constant.character.lhpconstant",
              "match": "\\b(gs|sw|cw|aEW|Gf|MH|MT|WH|MW|WW|MZ|WZ|vev|v|FeynmanGauge|feynmangaugerules|ee|Forward|Straight)\\b"
            },
            {
              "name": "constant.character.lhpparticle",
              "match": "\\b(G0|GP|GPbar|A|W|Wbar|Z|Phi|Phibar|G|H|Wi)\\b"
            }
          ]
        }
      }
    },
    "strings": {
      "name": "string.quoted.double.lhp",
      "begin": "'",
      "end": "'",
      "patterns": [
        {
          "name": "constant.character.escape.lhp",
          "match": "\\\\."
        }
      ]
    },
    "comments": {
      "name": "comment.line.percentage.lhp",
      "match": "%.*$"
    },
    "operators": {
      "patterns": [
        {
          "name": "constant.character.operators",
          "match": "==="
        },
        {
          "name": "constant.character.operators",
          "match": "(\\->|\\:=|\\/\\.|==|\\:>)"
        },
        {
          "name": "constant.character.operators",
          "match": "[\\[\\]\\{\\}\\(\\)\\+\\-\\*\\/\\^\\|]"
        }
      ]
    }
  }
}
